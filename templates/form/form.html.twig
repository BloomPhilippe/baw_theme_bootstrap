{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

{% if attributes.id == 'user-login-form' %}
  <div class="container">
{% endif %}
  <div class="row row-{{ attributes.id }}">
    <div class="col-sm-12 col-md-12">
      {% if attributes.id == 'contact-message-support-technique-par-courriel-form' %}
        <p>
          {{ 'Nous sommes Ã©galement disponible par mail via ce formulaire de contact.'|trans }}
        </p>
      {% endif %}
      <form{{ attributes }}>
        <div class="row">
          {{ children }}
          <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
      </form>
    </div>
  </div>
{% if attributes.id == 'user-login-form' %}
</div>
{% endif %}


